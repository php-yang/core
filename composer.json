{
    "name": "php-yang/core",
    "description": "Yang's core.",
    "keywords": [
        "php",
        "framework",
        "yang"
    ],
    "homepage": "http://github.com/php-yang/core",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "pengzhile",
            "email": "pengzhile@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Yang\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yang\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
